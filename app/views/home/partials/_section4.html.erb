<section class="mt-32 bg-no-repeat bg-cover" style="background-image: url(system/light_grey_background.png);">
  <div class="sm:grid sm:w-full grid-cols-12">
    <div class="mt-32 col-span-1 relative">
      <%= image_pack_tag 'yellow_circle_outline_opacity.svg', class: "w-24" %>
      <div class="absolute top-0 mt-40">
        <%= image_pack_tag 'light_grey_3_lines_horizontal.svg'%>
      </div>
    </div>
    <div class="col-span-10 flex flex-wrap">
      <div class="sm:flex sm:w-full sm:mt-16 mx-20 sm:justify-center sm:flex-wrap">
        <div class="w-full flex justify-center">
          <div class="flex border rounded-lg shadow justify-between items-center sm:mr-24 py-1 w-3/4 bg-white">
            <div class="flex justify-center items-center w-full">
              <label class="ml-3 mr-5">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-5 text-gray-600">
                  <path fill-rule="evenodd" d="M8 4a4 4 0 100 8 4 4 0 000-8zM2 8a6 6 0 1110.89 3.476l4.817 4.817a1 1 0 01-1.414 1.414l-4.816-4.816A6 6 0 012 8z" clip-rule="evenodd" />
                </svg>
              </label>
              <input class="my-2 w-full focus:outline-none" type="text" name="" placeholder="Find a DPC/Concierge doctor near you">
            </div>
            <div>
              <button class="bg-doctor-yellow text-gray-100 mr-4 rounded-md px-8 py-1">Search</button>
            </div>
          </div>
        </div>
        <div class="w-full mt-20 text-center">
          <h2 class="text-4xl font-hurmebold text-doctor-blue">Content placement of Senario-2 explainer</h2>
          <p class=" inline">Omit this, if there's no Additional Content for this line </p>
        </div>
        <div class="w-full mt-20 pb-48">
          <div class="w-full flex flex-wrap justify-between">
            <div class="sm:w-1/4 bg-gray-100 rounded-tl-2lg rounded-br-2lg px-8 py-12 flex items-center shadow-md">
              <h3 class="font-hurmebold text-xl text-doctor-blue inline-block align-middle">
                What is direct primary care medicine
              </h3>
            </div>
            <div class="sm:w-1/4 bg-gray-100 rounded-tl-2lg rounded-br-2lg px-8 py-12 flex items-center shadow-md">
              <h3 class="font-hurmebold text-xl text-doctor-blue ">
                What is Concierge medicine
              </h3>
            </div>
            <div class="sm:w-1/4 bg-gray-100 rounded-tl-2lg rounded-br-2lg px-8 py-12 flex items-center shadow-md">
              <h3 class="font-hurmebold text-xl text-doctor-blue ">
                Differebce between DPC and Concierge
              </h3>
            </div>
          </div>
          <div class="w-full flex fflex-wrap justify-between mt-12">
            <div class="sm:w-1/4 bg-gray-100 rounded-tl-2lg rounded-br-2lg px-8 py-12 flex items-center shadow-md">
              <h3 class="font-hurmebold text-xl text-doctor-blue ">
                Const of Concierge medicine
              </h3>
            </div>
            <div class="sm:w-1/4 bg-gray-100 rounded-tl-2lg rounded-br-2lg px-8 py-12 flex items-center shadow-md">
              <h3 class="font-hurmebold text-xl text-doctor-blue ">
                Find your Concierge doctor
              </h3>
            </div>
            <div class="sm:w-1/4 bg-gray-100 rounded-tl-2lg rounded-br-2lg px-8 py-12 flex items-center shadow-md">
              <h3 class="font-hurmebold text-xl text-doctor-blue ">
                Find your DPC doctor
              </h3>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-span-1 grid">
      <div class="flex items-end relative justify-end pb-8">
        <div class="">
          <%= image_pack_tag 'light_yellow_slant.svg', class: "w-16 float-right" %>
        </div>
        <div class="absolute left-0 bottom-0 ml-6 mb-4">
          <%= image_pack_tag 'vector_plus_with_side_dots.svg', class: "w-16" %>
        </div>
      </div>
    </div>
  </div>
</section>
