<div class="w-full mt-2">
  <span class="border border-gray-400 font-hurmebold text-sm tracking-wide p-1 rounded">Filter by</span>
</div>
<div class="w-full border-r pr-4 border-gray-200 mt-6">
  <div class="w-full mt-4 border-b pb-6 text-sm">
    <label class="tracking-wide font-hurmebold">Degree</label>
    <div class="mt-2  text-gray-600">
      <div class="grid gap-1 grid-cols-3">
        <div class="filter-checkbox-button">
          <input type="checkbox" id="degree1" name="degree" value="md">
          <label for="degree1">MD</label>
        </div>
        <div class="filter-checkbox-button">
          <input type="checkbox" id="degree2" name="degree" value="dom">
          <label for="degree2">DOM</label>
        </div>
        <div class="filter-checkbox-button">
          <input type="checkbox" id="degree3" name="degree" value="np">
          <label for="degree3">NP</label>
        </div>
        <div class="filter-checkbox-button">
          <input type="checkbox" id="degree4" name="degree" value="fnp">
          <label for="degree4">FNP</label>
        </div>
        <div class="filter-checkbox-button">
          <input type="checkbox" id="degree4" name="degree" value="armp">
          <label for="degree4">ARMP</label>
        </div>
        <div class="filter-checkbox-button">
          <input type="checkbox" id="degree4" name="degree" value="pa">
          <label for="degree4">PA</label>
        </div>
      </div>
    </div>
  </div>
  <div class="w-full mt-4 border-b pb-6 text-sm  border-gray-200">
    <label class="tracking-wide font-hurmebold">Specialist</label>
    <div class="relative mt-1">
      <select class="block appearance-none w-full bg-white border border-gray-400 hover:border-gray-500 px-4 py-2 pr-8 rounded-lg shadow leading-tight focus:outline-none focus:shadow-outline text-gray-600">
        <option>Specialist Dropdown</option>
        <option>Option 2</option>
        <option>Option 3</option>
      </select>
      <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2">
        <svg class="fill-current h-4 w-4  text-gray-500" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20"><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z"/></svg>
      </div>
    </div>
  </div>
  <div class="w-full mt-4 border-b pb-6 text-sm  border-gray-200">
    <label class="tracking-wide font-hurmebold">Practice Type</label>
    <div class="mt-2">
      <div class="w-full flex">
        <div class="w-3/4">
          DPC Doctor
        </div>
        <div class="w-1/4">
          <input type="radio" class="form-radio" name="accountType" value="personal">
        </div>
      </div>
      <div class="w-full flex mt-1">
        <div class="w-3/4">
          Concierge Doctor
        </div>
        <div class="w-1/4">
          <input type="radio" class="form-radio" name="accountType" value="personal">
        </div>
      </div>
      <div class="w-full flex mt-1">
        <div class="w-3/4">
          Both
        </div>
        <div class="w-1/4">
          <input type="radio" class="form-radio" name="accountType" value="personal">
        </div>
      </div>
    </div>
  </div>
  <div class="w-full mt-4 border-b pb-6 text-sm  border-gray-200">
    <label class="tracking-wide font-hurmebold">Location</label>
    <div class="mt-2 w-full">
      <input type="text" name="location" placeholder="City/Zip/State" class="w-full border px-3 py-1 border-gray-500 rounded-lg">
    </div>
  </div>
  <div class="w-full mt-4 border-b pb-6 text-sm  border-gray-200">
    <label class="tracking-wide font-hurmebold">Hourse</label>
    <div class="mt-2">
      <div class="w-full flex">
        <div class="w-3/4">
          Part Time
        </div>
        <div class="w-1/4">
          <div class="relative inline-block w-12 mr-2 align-middle select-none transition duration-200 ease-in">
            <input type="checkbox" name="part_time" id="part_time" class="toggle-checkbox absolute block w-5 h-5 rounded-full bg-white border-4 appearance-none cursor-pointer focus:outline-none"/>
            <label for="part_time" class="toggle-label block overflow-hidden h-5 rounded-full bg-gray-300 cursor-pointer"></label>
          </div>
        </div>
      </div>
      <div class="w-full flex mt-2">
        <div class="w-3/4">
          FullName Time
        </div>
        <div class="w-1/4">
          <div class="relative inline-block w-12 mr-2 align-middle select-none transition duration-200 ease-in">
            <input type="checkbox" name="full_time" id="full_time" class="toggle-checkbox absolute block w-5 h-5 rounded-full bg-white border-4 appearance-none cursor-pointer focus:outline-none"/>
            <label for="full_time" class="toggle-label block overflow-hidden h-5 rounded-full bg-gray-300 cursor-pointer"></label>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="w-full mt-4 border-b pb-6 text-sm  border-gray-200">
    <label class="tracking-wide font-hurmebold">Visa Sponsership</label>
    <div class="mt-2">
      <div class="w-full flex">
        <div class="w-3/4">
          Yes
        </div>
        <div class="w-1/4">
          <input type="radio" class="form-radio" name="visa_sponsership" value="personal">
        </div>
      </div>
      <div class="w-full flex mt-1">
        <div class="w-3/4">
          No
        </div>
        <div class="w-1/4">
          <input type="radio" class="form-radio" name="visa_sponsership" value="personal">
        </div>
      </div>
    </div>
  </div>
</div>
